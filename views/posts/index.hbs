 <form action='/sessions?_method=DELETE' method='POST'>
      <input type="submit" value="Log Out">
    </form>
<h1>Timeline</h1>
<ul class="posts">
  {{#each posts}}
  <li>{{this.message}} {{this.createdAt}} {{this.likes}}</li>
 
 <form action=`/posts/${this._id}/likes` method='POST'>
   <input type="submit" value="Me likey">
  </form>

  {{/each}}
</ul>

<a class="new-post-link" href="/posts/new">New post</a>
